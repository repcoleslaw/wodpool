{
  "name": "wodpool-server",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "preset": "q3-test-utils",
    "roots": [
      "lib",
      "tests"
    ]
  },
  "resolutions": {
    "axios": "^0.21.1",
    "mongoose": "^6.1.0"
  },
  "scripts": {
    "postinstall": "yarn-deduplicate",
    "test": "jest --runInBand --silent --detectOpenHandles --forceExit",
    "lint": "eslint  --ext .js lib/**",
    "start:web": "node --optimize_for_size lib/index.js",
    "start:worker": "node lib/worker.js",
    "develop:web": "nodemon lib/index.js",
    "develop:worker": "nodemon lib/worker.js"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "mongoose": "^6.1.0",
    "q3-api": "^2.0.1",
    "q3-core-composer": "^2.0.1",
    "q3-core-mailer": "^2.0.1",
    "q3-core-responder": "^2.0.1",
    "q3-core-scheduler": "^2.0.1",
    "q3-core-session": "^2.0.1",
    "yarn-deduplicate": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-node-3merge": "^1.2.1",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "q3-test-utils": "^2.0.1",
    "supertest": "^6.1.6"
  }
}
